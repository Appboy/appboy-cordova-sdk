<?xml version='1.0' encoding='utf-8'?>
<widget id="com.appboy.hellocordova" version="2.3.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HelloCordova</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-appboy" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="com.appboy.api_key" value="7ef72382-b92f-4258-9be9-9e19e92f3bf1" />
        <preference name="com.appboy.android_automatic_push_registration_enabled" value="true" />
        <preference name="com.appboy.android_gcm_sender_id" value="901477453852" />
        <preference name="com.appboy.android_default_session_timeout" value="5" />
        <preference name="com.appboy.android_handle_push_deep_links_automatically" value="true" />
        <preference name="com.appboy.android_log_level" value="0" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="com.appboy.api_key" value="06a4b5d1-099b-4910-9604-583ac8f2dc10" />
        <preference name="com.appboy.ios_disable_automatic_push_registration" value="false" />
    </platform>
</widget>
